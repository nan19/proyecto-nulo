proc hola (ent: entero i, entero j; ent-sal: entero k, real l; sal: booleano b)
	i:= i+j;
	k:=1;	
finp

comienzo	
entero a;			
booleano b;	
real z;
z:=1;	
a := 1*2 + 3*4 - 5 div 6 + 7 div 8 + 9 mod 10;
[1] entero ar;
[] entero arr := [1];
hola(1,2,3);
si (a > 0)
    a:= 1;
    entero b;
	hacer (3>2)
		entero c:=4;
	finh
	hola(1,2);
	si (3<2)
		z:=0;
	sino
		z:=1;
	fins
fins
a:=0;
fin